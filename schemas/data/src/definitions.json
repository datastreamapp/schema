    "string": {
      "type": "string",
      "pattern": "^[\\p{L}\\p{N}\\p{P}\\p{S} ]+$",
      "maxLength": 4000
    },
    "string-multiline": {
      "type": "string",
      "pattern": "^[\\p{L}\\p{N}\\p{P}\\p{S} \\r\\n]+$",
      "maxLength": 4000
    },
    "date": {
      "errorMessage": {
        "format": "error-date-format"
      },
      "type": "string",
      "format": "date",
      "minLength": 10,
      "maxLength": 10
    },
    "time-loose": {
      "errorMessage": {
        "pattern": "error-time-loose-pattern"
      },
      "type": "string",
      "pattern": "^(2[0-3]|[01]?[0-9]):[0-5]?[0-9](:[0-5]?[0-9](\\.[0-9]{1,3})?)?$",
      "minLength": 3,
      "maxLength": 12
    },
    "time-strict": {
      "errorMessage": {
        "pattern": "error-time-strict-pattern"
      },
      "type": "string",
      "pattern": "^(2[0-3]|[01][0-9]):[0-5][0-9](:[0-5][0-9](\\.[0-9]{1,3})?)?$",
      "minLength": 5,
      "maxLength": 12
    },
    "timezone-loose": {
      "errorMessage": {
        "pattern": "error-timezone-loose-pattern"
      },
      "type": "string",
      "pattern": "^(Z|[+-]([0-9]|0[0-9]|1[0-9]|2[0-4]):?[0-5][0-9])$",
      "minLength": 1,
      "maxLength": 6
    },
    "timezone-strict": {
      "errorMessage": {
        "pattern": "error-timezone-strict-pattern"
      },
      "type": "string",
      "pattern": "^(Z|[+-](0[0-9]|1[0-9]|2[0-4]):[0-5][0-9])$",
      "minLength": 1,
      "maxLength": 6
    },
    "csv-injection": {
      "type": "string",
      "pattern": "^[^ \\x00\\t\\n\\r=+\\-@]{1}.*$",
      "maxLength": 4000
    },
    "trim": {
      "type": "string",
      "pattern": "^([^\\s]+.*|.*[^\\s]+)$",
      "maxLength": 4000
    },
    "cr-char": {
      "description": "carrage return",
      "type": "string",
      "pattern": "^.*[\\u00D0].*$",
      "maxLength": 4000
    },
    "tab-char": {
      "description": "tab",
      "type": "string",
      "pattern": "^.*[\\u0009].*$",
      "maxLength": 4000
    },
    "space-char": {
      "description": "no break space, replace with \\u0020",
      "type": "string",
      "pattern": "^.*[\\u00A0].*$",
      "maxLength": 4000
    },
    "invisible-char": {
      "description": "Collection of invisible chars that shouldn't be used (exclude space, newline, carrage return). Replace with nothing. https://invisible-characters.com",
      "type": "string",
      "pattern": "^.*[\\u00AD\\u034F\\u061C\\u115F\\u160F\\u17B4\\u17B5\\u180E\\u2000-\\u200F\\u202F\\u205F-\\u2064\\u206A-\\u206F\\u2800\\u3000\\u3164\\uFEFF\\uFFA0].*$",
      "maxLength": 4000
    }
  }
